{
  "env": {
    "node": true,
  },
  "globals": {
    "$": true,
    "_": true,
    "atob": true,
    "btoa": true,
    "chars": true,
    "jQuery": true,
    "window": true
  },
  "rules": {
    "camelcase": 0,
    "consistent-return": 0,
    "curly": 2,
    "eol-last": 0,
    "max-depth": [2, 5],
    "max-nested-callbacks": [1,3],
    "max-params": [1, 5],
    "no-path-concat": 0,
    "no-underscore-dangle": 1,
    "no-undef": 2,
    "no-unused-vars": 2,
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "strict": 0
  }
}
