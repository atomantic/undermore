{
  "env": {
    "node": true,
  },
  "globals": {
  },
  "rules": {
    "brace-style": [1, "1tbs", { "allowSingleLine": true }],
    "camelcase": 0,
    "comma-spacing": [2, {"before": false, "after": true}],
    "complexity": [1, 7],
    "consistent-return": 0,
    "curly": 2,
    "eol-last": 0,
    "max-depth": [2, 5],
    "max-nested-callbacks": [1,3],
    "max-params": [1, 5],
    "max-statements": [],
    "no-multi-spaces": [2, 1],
    "no-path-concat": 0,
    "no-underscore-dangle": 1,
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "strict": 0
  }
}
